{
   "name": "javascript-development-environment",
   "version": "1.0.0",
   "description": "JavaScript development environment Pluralsight course by Cory House",
   "scripts": {
      "prestart": "babel-node buildScripts/startMessage.js",
      "start": "npm-run-all --parallel open:src lint:watch test:watch start-mockapi",
      "open:src": "babel-node buildScripts/srcServer.js",
      "lint": "esw webpack.config.* src buildScripts --color",
      "lint:watch": "npm run lint -- --watch",
      "share-lt": "lt -h \"http://serverless.social\" -p 3000",
      "share-ng": "ngrok http 3000",
      "test": "mocha --reporter progress buildScripts/testSetup.js \"src/**/*.test.js\"",
      "test:watch": "npm run test -- --watch",
      "generate-mock-data": "babel-node buildScripts/generateMockData.js",
      "prestart-mockapi": "npm run generate-mock-data",
      "start-mockapi": "json-server --watch src/api/db.json  --port 3001"
   },
   "author": "Arun S. Rajagopalan",
   "license": "MIT",
   "dependencies": {
      "whatwg-fetch": "^3.0.0"
   },
   "devDependencies": {
      "@types/express": "^4.17.6",
      "babel-cli": "^6.26.0",
      "babel-core": "^6.26.3",
      "babel-loader": "^7.1.5",
      "babel-preset-env": "^1.7.0",
      "babel-preset-latest": "^6.24.1",
      "babel-register": "^6.26.0",
      "chai": "^4.2.0",
      "chalk": "^4.0.0",
      "cheerio": "^0.22.0",
      "compression": "^1.7.4",
      "cross-env": "^7.0.2",
      "css-loader": "^3.5.3",
      "eslint": "^7.0.0",
      "eslint-plugin-import": "^2.20.2",
      "eslint-watch": "^6.0.1",
      "express": "^4.17.1",
      "extract-text-webpack-plugin": "^3.0.2",
      "html-webpack-plugin": "^4.3.0",
      "ignore-styles": "^5.0.1",
      "jsdom": "^16.2.2",
      "json-schema-faker": "^0.4.7",
      "json-server": "^0.16.1",
      "localtunnel": "^2.0.0",
      "mocha": "^7.1.2",
      "nock": "^12.0.3",
      "npm-run-all": "^4.1.5",
      "numeral": "^2.0.6",
      "open": "^7.0.3",
      "rimraf": "^3.0.2",
      "style-loader": "^1.2.1",
      "surge": "^0.21.3",
      "webpack": "^4.43.0",
      "webpack-dev-middleware": "^3.7.2",
      "webpack-hot-middleware": "^2.25.0",
      "webpack-md5-hash": "^0.0.6"
   }
}
